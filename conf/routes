# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                                       controllers.Application.index()



GET        /address                                controllers.UserCtrl.address()
GET        /addressnew                             controllers.UserCtrl.addressnew()
GET        /carded                                 controllers.UserCtrl.carded()
GET        /install                                controllers.UserCtrl.install()
GET        /list                                   controllers.ProductsCtrl.list()
GET        /detail                                 controllers.ProductsCtrl.detail()
GET        /all                                    controllers.ShoppingCtrl.all()
GET        /appraise                               controllers.ShoppingCtrl.appraise()
GET        /assess                                 controllers.ShoppingCtrl.assess()
GET        /coupon                                 controllers.ShoppingCtrl.coupon()
GET        /delivered                              controllers.ShoppingCtrl.delivered()
GET        /drawback                               controllers.ShoppingCtrl.drawback()
GET        /fightgroups                            controllers.ShoppingCtrl.fightgroups()








#by howen
GET     /setting                                controllers.ShoppingCtrl.setting()
GET     /pin/list                               controllers.ShoppingCtrl.pinList()
GET     /login                                  controllers.ShoppingCtrl.login()
GET     /logistic                               controllers.ShoppingCtrl.logistic()
GET     /means                                  controllers.ShoppingCtrl.means()
GET     /myView                                 controllers.ShoppingCtrl.myView()
GET     /nav                                    controllers.ShoppingCtrl.navBar()
GET     /obligati                               controllers.ShoppingCtrl.obligati()
GET     /orders                                 controllers.ShoppingCtrl.orders()
GET     /user/reset/passwd                      controllers.ShoppingCtrl.resetPasswd()
GET     /settle                                 controllers.ShoppingCtrl.settle()
GET     /regist                                 controllers.ShoppingCtrl.regist()
GET     /tickling                               controllers.ShoppingCtrl.tickling()
GET     /cart                                   controllers.ShoppingCtrl.cart()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


